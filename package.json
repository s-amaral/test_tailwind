    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "npx jest --passWithNoTests",
    "format": "npm run eslint -- --fix && npm run stylelint -- --fix && npm run prettier",
    "prettier": "npx prettier --write src public config scripts .storybook",
    "prettier-check": "npx prettier --check src public config scripts .storybook",
    "eslint": "npx eslint src config scripts .storybook --ext .js,.ts,.tsx",
    "eslint:github": "npm run eslint -- --config ./.eslintrc.github_actions.js",
    "stylelint": "npx stylelint src/**/*.{css,scss,ts,tsx}",
    "prepush": "npm run eslint:github && npx tsc && npm run prettier-check && npm run stylelint && npm run depcruise && npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "depcruise": "NODE_ENV=development npx depcruise --config .dependency-cruiser.js src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
